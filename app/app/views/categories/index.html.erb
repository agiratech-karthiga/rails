
<table>
  <tr>
    <th>Id</th>
    <th>Name</th>
    <th>created_at</th>
    <th>updated_at</th>
  </tr>

  <% @categories.each do |category| %>
    <tr>
    <td><%=category.id%></td>
      <td><%= category.name %></td>
        <td><%= category.created_at %></td>
        <td><%= category.updated_at %></td>
      <td><%= link_to 'show', category_path(category) %></td>
      <td><%= link_to 'destroy', category_path(category.try(:id)), method: :delete, data: { confirm: 'Are you sure?' } %>
      </td>
    </tr>
  <% end %>
</table>
